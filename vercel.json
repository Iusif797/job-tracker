{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run build",
        "installCommand": "npm install"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/static/css/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "text/css"
      },
      "dest": "/static/css/$1"
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/favicon.ico"
    },
    {
      "src": "/logo192.png",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/logo192.png"
    },
    {
      "src": "/logo512.png",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/logo512.png"
    },
    {
      "src": "/manifest.json",
      "headers": {
        "content-type": "application/json"
      },
      "dest": "/manifest.json"
    },
    {
      "src": "/robots.txt",
      "headers": {
        "content-type": "text/plain"
      },
      "dest": "/robots.txt"
    },
    {
      "src": "/(.*)",
      "headers": {
        "content-type": "text/html; charset=utf-8"
      },
      "dest": "/index.html"
    }
  ]
} 